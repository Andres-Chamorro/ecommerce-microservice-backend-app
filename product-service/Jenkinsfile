        REGISTRY_IMAGE = "${DOCKER_REGISTRY}/${SERVICE_NAME}"
        
        // Minikube Configuration
        K8S_NAMESPACE = 'ecommerce-dev'
    }
    
    parameters {
        booleanParam(
            name: 'SKIP_TESTS',
            defaultValue: false,
            description: 'Saltar pruebas unitarias e integraciÃƒÆ’Ã†â€™Ãƒâ€ Ã¢â‚¬â„¢ÃƒÆ’Ã¢â‚¬Å¡Ãƒâ€šÃ‚Â³n'
        )
