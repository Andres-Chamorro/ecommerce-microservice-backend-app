            steps {
                script {
                    echo "ÃƒÆ’Ã†â€™Ãƒâ€šÃ‚Â°ÃƒÆ’Ã¢â‚¬Â¦Ãƒâ€šÃ‚Â¸ÃƒÆ’Ã‚Â¢ÃƒÂ¢Ã¢â‚¬Å¡Ã‚Â¬Ãƒâ€šÃ‚ÂÃƒÆ’Ã¢â‚¬Å¡Ãƒâ€šÃ‚Â¨ [DEV] Compilando ${SERVICE_NAME} con Maven..."
                    dir('product-service') {
                        sh 'mvn clean package -DskipTests'
                    }
                }
            }
        }
        
        stage('Unit Tests') {
            when {
                expression { params.SKIP_TESTS == false }
            }
            steps {
                script {
                    echo "ÃƒÆ’Ã†â€™Ãƒâ€šÃ‚Â°ÃƒÆ’Ã¢â‚¬Â¦Ãƒâ€šÃ‚Â¸ÃƒÆ’Ã¢â‚¬Å¡Ãƒâ€šÃ‚Â§ÃƒÆ’Ã¢â‚¬Å¡Ãƒâ€šÃ‚Âª [DEV] Ejecutando pruebas unitarias de ${SERVICE_NAME}..."
                    dir('product-service') {
                        sh 'mvn test -Dtest=*ServiceTest,*ResourceTest'
                    }
                }
            }
            post {
                always {
                    junit allowEmptyResults: true, testResults: 'product-service/target/surefire-reports/*.xml'
                }
            }
        }
        
        stage('Build Docker Image') {
            steps {
                script {
                    echo "ÃƒÆ’Ã†â€™Ãƒâ€šÃ‚Â°ÃƒÆ’Ã¢â‚¬Â¦Ãƒâ€šÃ‚Â¸ÃƒÆ’Ã¢â‚¬Å¡Ãƒâ€šÃ‚ÂÃƒÆ’Ã¢â‚¬Å¡Ãƒâ€šÃ‚Â³ [DEV] Construyendo imagen Docker de ${SERVICE_NAME}..."
                    sh """
                        # Construir imagen con Docker
                        docker build -t ${IMAGE_NAME}:dev-${BUILD_TAG} -f product-service/Dockerfile .
                        docker tag ${IMAGE_NAME}:dev-${BUILD_TAG} ${IMAGE_NAME}:dev-latest
                        
                        # Cargar imagen en Minikube usando minikube image load
                        # Reiniciar containerd si es necesario (por builds largos)
                        docker exec minikube systemctl restart containerd || true
                        sleep 5
                        
                        docker exec minikube sh -c "ctr -n k8s.io images pull docker.io/library/${IMAGE_NAME}:dev-${BUILD_TAG} || true"
                        docker save ${IMAGE_NAME}:dev-${BUILD_TAG} | docker exec -i minikube ctr -n k8s.io images import -
                        
                        echo "Imagen construida y cargada en Minikube: ${IMAGE_NAME}:dev-${BUILD_TAG}"
                    """
                }
            }
        }
        

        stage('Deploy to Minikube') {
            when {
                expression { params.SKIP_DEPLOY == false }
            }
            steps {
                script {
                    echo "ÃƒÆ’Ã†â€™Ãƒâ€šÃ‚Â¢ÃƒÆ’Ã¢â‚¬Â¹Ãƒâ€¦Ã¢â‚¬Å“ÃƒÆ’Ã¢â‚¬Å¡Ãƒâ€šÃ‚Â¸ÃƒÆ’Ã†â€™Ãƒâ€šÃ‚Â¯ÃƒÆ’Ã¢â‚¬Å¡Ãƒâ€šÃ‚Â¸ÃƒÆ’Ã¢â‚¬Å¡Ãƒâ€šÃ‚Â [DEV] Desplegando ${SERVICE_NAME} en Minikube..."
                    
