pipeline {
    agent any
    
    environment {
        // Java Configuration
        JAVA_HOME = '/usr/lib/jvm/java-17-openjdk-amd64'
        PATH = "/root/google-cloud-sdk/bin:${JAVA_HOME}/bin:${env.PATH}"
        
        // Service Configuration
        SERVICE_NAME = 'product-service'
        SERVICE_PORT = '8300'
        
        // Build Configuration
        MAVEN_OPTS = '-Xmx2048m'
        BUILD_TAG = "${env.BUILD_NUMBER}"
        
